	AssertionResult.setFailureMessage(txt);
	AssertionResult.setFailure(true);
}

eval(&apos;var json = &apos;+prev.getResponseDataAsString());


try {
	if (json.documents) {
		for (i=0; i&lt;json.documents.length; i++) {

			var doc_ID = json.documents[i][&quot;doc_ID&quot;];
			//var req = &apos;{&quot;by_doc_ID&quot;:false, &quot;by_resource_ID&quot;:true, &quot;request_IDs&quot;:[&quot;&apos;+escape(doc_ID)+&apos;&quot;]}&apos;;
